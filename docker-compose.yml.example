version: "3.9"

services:
  postgres:
    image: postgres:16
    container_name: notion_postgres
    ports:
      - "5432:5432"
    environment:
      POSTGRES_DB: notion_db
      POSTGRES_USER: postgres
      POSTGRES_PASSWORD: postgres
    volumes:
      - pgdata:/var/lib/postgresql/data

volumes:
  pgdata:
